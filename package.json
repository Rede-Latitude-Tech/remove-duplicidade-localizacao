{
  "name": "remove-duplicidade-localizacao",
  "version": "0.1.0",
  "description": "Microserviço de detecção e eliminação de duplicidades de localização no CRM Latitude",
  "main": "dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push"
  },
  "keywords": [
    "deduplicacao",
    "localizacao",
    "crm",
    "latitude",
    "real-estate"
  ],
  "author": "Latitude CRM",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.0",
    "@prisma/client": "^6.0.0",
    "bullmq": "^5.0.0",
    "fastify": "^5.0.0",
    "ioredis": "^5.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "prisma": "^6.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  }
}
